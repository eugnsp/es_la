	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build es_la",
			"type": "shell",
			"command": "cmake .. && make -j4",
			"options": {"cwd": "${workspaceRoot}/build"},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [
				"$gcc"
			],
			"presentation": {
                "clear": true
            }
		}
	]
}
